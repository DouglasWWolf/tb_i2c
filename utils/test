source "registers.sh"

axireg $I2C_STATUS
axireg $I2C_STATUS

axireg $I2C_DEV_ADDR 0x4B
axireg $I2C_REG_NUM  0x00
axireg $I2C_READ_LEN 2

echo "Just issued read, status is"
axireg $I2C_STATUS
sleep 1

echo "Slept after read, status is"
axireg $I2C_STATUS

echo "Result is"
axireg $I2C_RX_DATA

